.btn-primary,.btn-primary:hover,.ui-state-active a.btn{background-color:#69c;border-color:#5c8ab8 #5c8ab8 #4c7399;border-color:rgba(0,0,0,0.1) rgba(0,0,0,0.1) rgba(0,0,0,0.25);color:#fff;text-shadow:0 -1px 0 rgba(0,0,0,0.25)}
