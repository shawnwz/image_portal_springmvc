.menu-dialog .ui-dialog-titlebar,.iframe-dialog .ui-dialog-titlebar{display:none}
.iframe-dialog{background-color:#dbdbdb;padding:8px 0;-moz-border-radius:5px;-webkit-border-radius:5px;border-radius:5px;-moz-box-shadow:0 0 8px #000;-webkit-box-shadow:0 0 8px #000;box-shadow:0 0 8px #000}
#iframe-loading{position:absolute;left:50%;top:50%;margin:-15px 0 0 -15px;width:31px;height:31px;background:url("../images/my-account/ajax-loader_31x31.gif") no-repeat 0 0}
.no-header .ui-dialog-titlebar{display:none}
.modal-dialog{background:#DBDBDB;color:#1A1A1A}
.sticky-footer{position:fixed;bottom:0;left:0;right:0;-webkit-border-radius:0!important;-moz-border-radius:0!important;border-radius:0!important;z-index:101}
.alert{background-color:#333;border:0;-webkit-border-radius:5px;-moz-border-radius:5px;border-radius:5px;padding:10px 26px 10px 10px;text-shadow:none}
.alert{color:#999}
.alert-heading{color:#fff}
.alert .close{right:-16px}
.alert-success{background-color:#dcf5de}
