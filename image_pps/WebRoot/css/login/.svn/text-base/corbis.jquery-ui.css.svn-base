.ui-dialog{position:absolute}
.ui-dialog-titlebar{background:#DBDBDB;padding:10px;-webkit-border-top-left-radius:7px!important;-webkit-border-top-right-radius:7px!important;-moz-border-radius-topleft:7px!important;-moz-border-radius-topright:7px!important;border-top-left-radius:7px!important;border-top-right-radius:7px!important;-webkit-border-bottom-left-radius:0!important;-webkit-border-bottom-right-radius:0!important;-moz-border-radius-bottomleft:0!important;-moz-border-radius-bottomright:0!important;border-bottom-left-radius:0!important;border-bottom-right-radius:0!important;overflow:auto}
.ui-dialog-title{float:left}
.ui-dialog-titlebar-close{width:15px;height:15px;cursor:pointer;float:right;border-radius:0!important;-webkit-border-radius:0!important;background:url(/packages/images/modal-close.gif) no-repeat}
.ui-widget-overlay{background-color:#000;position:absolute;top:0;left:0;opacity:.4;filter:alpha(opacity = 40)}
.ui-transparent-overlay{opacity:0;filter:alpha(opacity = 0)}
.ui-dialog-buttonpane{-webkit-border-bottom-left-radius:7px;-webkit-border-bottom-right-radius:7px;-moz-border-radius-bottomleft:7px;-moz-border-radius-bottomright:7px;border-bottom-left-radius:7px;border-bottom-right-radius:7px;background:#DBDBDB;padding:10px 0;overflow:auto}
.ui-dialog-buttonset{float:right}
.ui-button{-webkit-border-radius:.4em;-moz-border-radius:.4em;border-radius:.4em;-webkit-box-shadow:0 1px 1px rgba(255,255,255,.2);-moz-box-shadow:0 1px 1px rgba(255,255,255,.2);box-shadow:0 1px 1px rgba(255,255,255,.2);color:#FFF;cursor:pointer;display:inline-block;line-height:1em;outline:0;padding:5px 10px;text-align:center;text-decoration:none;margin:0 5px}
.ui-button.medium{height:20px;line-height:20px;color:#f1f1f1;vertical-align:top;padding:0 10px;font-size:12px}
.ui-corner-all{-webkit-border-radius:7px;-moz-border-radius:7px;border-radius:7px}
.ui-autocomplete{margin-top:10px;background-color:#DDD;width:318px!important;-moz-box-shadow:0 0 5px #000;-webkit-box-shadow:0 0 5px #000;box-shadow:0 0 5px #000;z-index:15!important}
.ui-menu{list-style-type:none}
.ui-menu-item{background:#F1F1F1;font-weight:bold;padding:0 5px;cursor:pointer;line-height:20px}
.ui-menu-item a{width:100%;color:#1A1A1A;cursor:default;cursor:pointer;text-decoration:none;display:inline-block}
.ui-menu-item a:hover,.ui-autocomplete .ui-state-hover{background:#ccc;margin:0 -5px;padding:0 5px;-webkit-border-radius:0;-moz-border-radius:0;border-radius:0}
.autocomplete-header{height:10px}
.autocomplete-turn-off{padding:5px;font-size:10px;color:#69C;cursor:pointer;text-decoration:none}