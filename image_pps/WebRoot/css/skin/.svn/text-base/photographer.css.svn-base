.hero-image{margin-right:20px}
#enlarged-image{clear:both;border:1px solid #333;width:650px;vertical-align:middle;text-align:center;;margin:0 auto;}
#enlarged-image img{vertical-align:bottom}

#hero-toolbar{height:20px;background:#222;width:652px;line-height:19px;margin-bottom:16px}
#hero-toolbar span.hero-credit{font-size:10px;padding-left:4px}
#hero-toolbar span a{color:#6699CC;}
.browse-filmstrip .header{display:inline-block;background:0;height:auto;width:652px;min-width:652px;margin-bottom:4px}
.browse-filmstrip .header p{text-align:left;}
.browse-filmstrip .header h2{font-size:18px;clear:left;margin-bottom:6px}
.browse-filmstrip{clear:left;padding-bottom:16px;border-top:solid 1px #333;padding-top:16px;margin-top:16px;display:block}
.filmstrip-navigation{margin-top:30px;float:right;overflow:auto}
.filmstrip-navigation a{float:right;color:#6699CC;}
.filmstrip-navigation a.filmstrip-nav-link{margin-right:10px}
.filmstrip-navigation a.filmstrip-next{background:url('../images/pre.png') -20px 0 no-repeat;height:20px;width:20px;border-bottom:0}
.filmstrip-navigation a.filmstrip-next:hover{background-position:-20px -20px}
.filmstrip-navigation a.filmstrip-prev{background:url('../images/pre.png') 0 0 no-repeat;height:20px;width:20px;margin-right:5px;border-bottom:0}
.filmstrip-navigation a.filmstrip-prev:hover{background-position:0 -20px}
.filmstrip-thumb{width:170px;height:170px;margin-bottom:14px;position:relative;opacity:.8}
.truncatedText{overflow:hidden}
.filmstrip-thumb:hover{opacity:1.0}
.reflect canvas{padding-top:170px;position:relative;z-index:-1}
.filmstrip-thumb-info{position:relative;z-index:1;color:#707070;text-align: left;}
.filmstrip-thumb-info li a{color:#6699CC}

.jcarousel-skin-corbisimages{clear:both;height:260px;margin-bottom:50px}
.jcarousel-skin-corbisimages .jcarousel-container{padding:10px 0}
.jcarousel-skin-corbisimages .jcarousel-direction-rtl{direction:rtl}
.jcarousel-skin-corbisimages .jcarousel-container-horizontal{width:990px}
.jcarousel-skin-corbisimages .jcarousel-clip{overflow:hidden}
.jcarousel-skin-corbisimages .jcarousel-clip-horizontal{width:990px}
.jcarousel-skin-corbisimages .jcarousel-item{display:table-cell;position:relative}
.jcarousel-skin-corbisimages .jcarousel-item img{max-height:170px;max-width:170px;opacity:.9;filter:alpha(opacity=70);position:absolute;left:0;bottom:0}
.jcarousel-skin-corbisimages .jcarousel-item img:hover{filter:alpha(opacity=100)}
.jcarousel-skin-corbisimages .jcarousel-item-horizontal{margin-left:0;margin-right:35px;width:170px}
.jcarousel-skin-corbisimages .jcarousel-direction-rtl .jcarousel-item-horizontal{margin-left:10px;margin-right:0}
.jcarousel-skin-corbisimages .jcarousel-item-placeholder{background:#fff;color:#000}
#browse-header{background:0;height:auto;margin-bottom:8px}
#browse-header h1{padding-bottom:10px;font-size:32px;font-weight:normal}
#browse-header a{font-weight:bold;font-size:14px;color:#6699CC;}
#browsePage{width:990px;padding:20px;display:block}
#promoBanner{height:100px;width:100%;clear:both}
#quicklinks{clear:both}
#search-header-menu{border-bottom:0}
.disabled {opacity: 0.4;}
