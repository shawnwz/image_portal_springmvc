<script>
$(document).ready(function(){
	
	var n = ${template};
	if(n != null)
	{
		$("#thumb_big").attr("src","webres/upload/"+n.thumb_big.src);
	}
	
	
	var map = eval(${phMap});
	var count = ${count};
	var html = '<div class="fontGrey">热门摄影师</div>';
	for(var i = 0;i<count;i++)
	{
		html += '<a href="photographer?pname='+map[i]+'">'+map[i]+'</a>';
	}
	
	$(".photographer").html(html);
		
	loadImages();	
	pageImageCollection();
});

function loadImages()
	{
		$(".collectionGallery .galleryWrapDefault").empty();
		var paras="?cid=${id }&p=1";//search#cid=${id }
		var reqParas={n:25,key:"",sortType:4,ssid:""};
		var ajaxSearchUrl="search.do";
		$.ajax({
			type : "POST",
			url : ajaxSearchUrl + paras,// "search",
			data : reqParas,// {'key':$("#q").val(), 'n':n, 'ssid':searchSID},
			dataType : "json",
			success : function(data) {
				if (data.success) {
					var html ="";
					if(data.data != null)
					{

						var length = data.data.length;
						if(length > 12)
						{
							length = 12;
						}
						for(var i =0;i< length;i++)
						{
							html+='<div class="galleryThumbDefault">';
							html+='	<div class="imgWrap">';
							html+='	<a href="javascript:void(0)" onclick="goDetailByCidNoRela(\''+data.data[i].corbisId+'\',2)" ><img src="'+ data.data[i].imageHost+ 'THUMBNAIL/240/'+ data.data[i].path+ '" loading="true"></a>';
							html+='	</div>';
							html+='</div>';
							
						}

						//alert(html);
						$(".collectionGallery .galleryWrapDefault").html(html);
						  
					}

				} 
			},
			failed : function() {
				
			}
		});
	}
	
	function pageImageCollection(){
	var ps = 36;
	$("#tab1 .mySlideShowDiv").empty();
	var type = "";//$("#typeSearch").val();
	$.ajax({
		 type: "get",
		 url: "getPhotocollectionList",
		 data: "p="+1+"&ps="+ps+"&type="+type,
		 success: function(msg){
		      
		     var html ='<ul style="margin-left: 0px;">';
		     for (var i = 0; i < msg.data.length; i++) {
		     if(i>0)
		     {
		     	i--;
		     }
		      html +='<li>';
		      var end = i+3;
		      for(var h = i;h<end;h++)
		      {	
		     	
				      html +='<div class="mySlideLi">';
				      var end2 = i+3;
				      for(var j = i;j<end2;j++)
				      {		
				      		if(msg.data[j] != null)
				      		{
					      	  	var n = null;
						     	if(msg.data[i].template != "")
						     	{
						    		n = eval('(' +msg.data[i].template+')');//msg.data[i];
						    	}
								var item = msg.data[i];
								
								html+='<a href="photocollection'+item.name+'" class="collectionThumb">';
								html+='	<div class="leftImg">';
					            if(n != null)
					            {
									html+='	<img src="webres/upload/'+n.thumb4[4].src+'">';
								}
								html+='	</div>';
								html+='	<div class="rightInfo">';
								html+='	<div class="name">'+item.name+'</div>';
								html+='	<div class="tag">'+item.intro+'</div>';
								html+='	</div>';
								html+='</a>';
																							    				            
					            i++;
				            }
				      }
				      html +='</div>';														    				            
				 }
				 html +='</li>';
				     	    	
				
			}
			
		      html +='</ul>';
		     	$("#tab1 .mySlideShowDiv").append(html);
		  	
		 }
	});
}
</script>