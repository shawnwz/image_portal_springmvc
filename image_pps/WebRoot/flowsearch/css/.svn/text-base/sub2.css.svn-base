	top: 14px;
}

.left_collection h6{
	font-size: 18px;
	color: white;
	position:relative;
	margin-top: 32px;
}

.left_collection p{
	font-size: 12px;
}

.thumb li p{
	position: absolute;
	color: white;
	top: 40%;
	width: 100%;
	text-align: center;
	display: none;
}


.simply-scroll-container { 
	position: relative;
}

.simply-scroll-clip { 
	position: relative;
	overflow: hidden;
}

.scroll_section{
	position: relative;
}

.scroll_button:before{
	content: "\e612";
	color: white;
}

.scroll_button_up:before{
	content: "\e621";
	color: white;
}

.scroll_button{
	position: absolute;
	z-index: 10000;
	font-family: "icomoon";
	bottom: 26px;
	right: 45%;
}

.scroll_button_up{
	position: absolute;
	z-index: 10000;
	font-family: "icomoon";
	top: 13px;
	right: 45%;
}

.left_left_collection a{
	display: block;
	width: 100%;
	height: 100%;
}

.simply-scroll-btn {
	font-family: "icomoon";
	color: white;
	position: absolute;
	width: 100%;
	height: 41px;
	bottom:20px;
	z-index:3;
	cursor: pointer;
	background: url(../images/details/bg_button_scroll.png);
	background-size: 100%;
	background-repeat: no-repeat;
	background-position:center;
}

.simply-scroll-btn-up{
	font-family: "icomoon";
	color: white;
	position: absolute;
	width: 100%;
	height: 41px;
	top: 0;
	z-index:3;
	cursor: pointer;
	background: url(../images/details/bg_button_scroll_up.png);
	background-size: 100%;
	background-repeat: no-repeat;
	background-position:center;
}

#scroller li:last-child{
	padding-bottom: 60px;
	background: #262626;
}

.vert { 
	width: 100%;
	height: 100%;
}

.vert .simply-scroll-clip {
	width: 100%;
	height: 100%;
    /*overflow: hidden;*/
}

.info_picture{
	width: 266px;
	margin: 20px auto 0 auto;
}

.info_picture_p{
	font-size: 18px;
}

span.info_picture_span:before{
	content: "\e605";
}

span.info_picture_span{
	float: left;
	font-family: 'icomoon';
	font-size: 24px;
	margin-left: 9px;
}

.info_picture p{
	margin-left: 50px;
	margin-top: 4px;
}

.info_liste p{
	margin-left: 12px;
}

.info_picture h1{
	text-align: left;
	position:relative;
	top: 40px;
	display: block;
	width: 80%;
	margin: 0;
	font-size: 28px;
	margin-bottom: 32px;
	color: white;
	clear: both;
	padding: 0;
}


button.info_picture_button{
	display: block;
	width: 100%;
	margin: 0 auto;
	text-align: center;
	padding: 18px;
	background: none;
	border: 1px solid #7d7d7d;
	cursor: pointer;
	z-index: 1000;
	background: #383838;
	color: #cccccc;
}

span.info_picture_plus{
	font-size: 40px;
	position: absolute;
	left: 16%;
	top: -6px;
	font-weight: lighter;
	text-align: center;
	color: white;
}

ul.info_liste{
	position: absolute;
	overflow: hidden;
	width: 100%;
	margin: 0 auto;
	bottom: 0;
}

.info_picture_plus{
	-webkit-transition: all .2s ease-out !important;
	-moz-transition: all .2s ease-out !important;
	-ms-transition: all .2s ease-out !important;
	-o-transition: all .2s ease-out !important;
	-khtml-transition: all .2s ease-out !important;
	transition: all .2s ease-out !important;
}

.small_info,
.big_info{
	background: #383838;
}

.info_picture_plus.active{

	-webkit-transform :rotate(45deg) !important;
	-webkit-transform-origin:center;
	-webkit-transition: all .2s ease-out !important;

	-moz-transform :rotate(45deg) !important;
	-moz-transform-origin:center;
	-moz-transition: all .2s ease-out !important;

	-ms-transform :rotate(45deg) !important;
	-ms-transform-origin:center;
	-ms-transition: all .2s ease-out !important;

	-o-transform :rotate(45deg) !important;
	-o-transform-origin:center;
	-o-transition: all .2s ease-out !important;

	-khtml-transform :rotate(45deg) !important;
	-khtml-transform-origin:center;
	-khtml-transition: all .2s ease-out !important;

	transform :rotate(45deg) !important;
	transform-origin:center;
	transition: all .2s ease-out !important;

}

span.download_info:before{
	content: "\e614";
}

.info_top p{
	display: block;
	width: 75%;
	text-align: center;
}

span.download_info{
	font-family: "icomoon";
	font-size: 24px;
