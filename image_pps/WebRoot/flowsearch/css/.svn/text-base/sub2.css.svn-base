@charset "utf-8";
/* CSS Document */

.pageImageSetDetail #sectionBtm {
    margin-top: 0px;
}
.border_top{
	border-top: 1px solid black;
}

.container h1{
	text-align: center;
	font-size: 48px;
	padding: 36px 0;
}

.container small{
	display: block;
	margin: 0 auto;
	width: 470px;
	font-size: 24px;
	text-align: center;
	padding-bottom: 56px;
}

.wrapper{
	width: 100%;
	overflow: hidden;
	margin-bottom: 50px;

	-webkit-transition : all .3s ease-out;
	-moz-transition : all .3s ease-out;
	-ms-transition : all .3s ease-out;
	-o-transition : all .3s ease-out;
	-khtml-transition : all .3s ease-out;
	transition : all .3s ease-out;
}

.button_collection_details{
	-webkit-transition : all .3s ease-out;
	-moz-transition : all .3s ease-out;
	-ms-transition : all .3s ease-out;
	-o-transition : all .3s ease-out;
	-khtml-transition : all .3s ease-out;
	transition : all .3s ease-out;
}

.small,
.middle,
.large{
	float: left;
	height:410px;
	position: relative;
	color: white;
	overflow: hidden;
}

.small{
	width: 25%;
	height: 100%;
	background:#383838;
}

.small h2,
.small p{
	font-size: 36px;
	padding: 26px;
	background: #323232;
	color: white;
}

.small p{
	background: none;
	font-size: 12px;
	line-height: 18px;
}

.middle{
    position: relative;
	width: 37.5%;

	-webkit-transition : all .3s ease-out;
	-moz-transition : all .3s ease-out;
	-ms-transition : all .3s ease-out;
	-o-transition : all .3s ease-out;
	-khtml-transition : all .3s ease-out;
	transition : all .3s ease-out;
}

.large{
	width:75%;

	-webkit-transition : all .3s ease-out;
	-moz-transition : all .3s ease-out;
	-ms-transition : all .3s ease-out;
	-o-transition : all .3s ease-out;
	-khtml-transition : all .3s ease-out;
	transition : all .3s ease-out;
}

.boxCollection img.image_box,
.boxCollection img.image_box{
	position: absolute;
	width: 110%;
    max-width: none;
	/*height:100%;*/
	top: 0;
}

.half img.image_box{
	width: 100%;
	/*height: auto;*/
}

.boxCollection:hover img.border,
.boxCollection:hover img.button_collection{
	-webkit-transform: none !important;
	-moz-transform: none !important;
	-o-transform: none !important;
	-ms-transform: none !important;
	-khtml-transform: none !important;
	transform: none !important;

	opacity: 1 !important;
}

.borderLine:after, .borderLine:before{
    content: " ";
    clear: both;
}
.borderLine:after{
    display: block;
    position: absolute;
    top: 30px;
    left: 30px;
    width: 30%;
    height: 30%;
    border-left: 1px solid #fff;
    border-top: 1px solid #fff;
    z-index: 3;
}
.borderLine:before{
    display: block;
    position: absolute;
    bottom: 30px;
    right: 30px;
    width: 30%;
    height: 30%;
    border-right: 1px solid #fff;
    border-bottom: 1px solid #fff;
    z-index: 3
}

.boxCollection img.border{
	width: 365px;
}

.boxCollection img.large{
	width: 790px;
	height: 340px;
}

.boxCollection img.button_collection{
	width: 50px;
}

a.no_margin_links{
	width: 100%;
	margin: 0;
	height: 410px;
	position: absolute;
}

a.half_links{
	width:100%;
	height:205px;
}

.middle img.border,
.large img.border{
	position: absolute;
	left: 30px;
	top: 30px;
}

.button_collection{
	display: block;
	margin: 133px auto 22px auto;
}

img.half_collection{
	display: block;
	margin: 50px auto 22px auto;
}

.middle small,
.large small{
	width: inherit;
	font-size: 18px;
	line-height: 32px;
	position: relative;
	z-index: 200;
}

.middle h3,
.large h3{
	text-align: center;
	font-size: 30px;
	line-height: 48px;
	position: relative;
	z-index: 200;
}

img.button_collection{
	position: relative;
	z-index: 200;
}


.half{
	height: 205px;
}

.button{
	width: 100%;
	position: absolute;
	bottom: 0;
	background: #323232;
}

.button button{
	padding: 22px 38px;
	background: #323232;
	border:none;
	color: white;
	font-size: 30px;
	border-right: 2px solid #383838;
}

.wrapper.heightPlus{
	height: 810px;
}

.mobile_landing{
	display: none;
}

@media(max-width: 1199px){

	.border{
		width: 290px !important;
	}

	.large img.border{
		width: 634px !important;
		height: 274px !important;
	}

	img.half_collection {
		display: block;
		margin: 24px auto 22px auto;
	}

	.button_collection {
		display: block;
		margin: 92px auto 22px auto;
	}
}

@media(max-width: 991px){

    .borderLine:after, .borderLine:before{
        display: none !important;
    }

	.button,
	.small p{
		display: none;
	}

	.wrapper{
		height: inherit;
		position: relative;
	}

	.small,
	.middle,
	.large{
		float: inherit;
		width: 100%;
		height: inherit;
		position: relative;
	}

	.image_box{
		position: relative;
		width: 100%;
	}

	.content_mobile_landing{
		overflow: hidden;
		position: absolute;
		bottom: 20px;
		left: 20px;
	}

	.mobile_landing{
		display: block;
	}

	.mobile_landing a{
		display: block;
		overflow: hidden;
	}

	.content_mobile_landing img{
		float: left;
		background: #323232;
		padding: 10px 15px;
	}

	.mobile_landing p{
		color: white;
		font-size: 32px;
		float: left;
		padding: 10px 15px;
		padding-bottom: 9px;
		background: rgba(50,50,50,.85);
	}

}


/***************** DETAILS ********************/




.details{
	width: 100%;
	padding: 0;
	overflow: hidden;
	position: relative;
}

.aside{
	background: #383838;
	width: 22%;
	height: 100%;
	left: 0;
	position: absolute;
	z-index: 1000;
}

.aside button{
	border: none;
	width: 49%;
	background: #323232;
	font-weight: bold;
	color: white;
	cursor: pointer;
}

span.left:before,
span.carrouselLeftSpan:before{
	content: "\e600";
}

span.right:before,
span.carrouselRightSpan:before{
	content: "\e601";
}

span.left,
span.right{
	font-family: 'icomoon';
	display: block;
	width: 100%;
	text-align: center;
	font-size: 16px;
	padding: 32px 0;
}

.carrouselDetails{
	width: 80%;
	float: left;
	background: #323232;
	position: absolute;
	bottom: 0;
	z-index: 2000;
}

.carrouselDetails button{
	height: 100%;
	background: #262626;
	color: white;
	border: none;
	font-weight: bold;
	position: absolute;
}

span.carrouselLeftSpan,
span.carrouselRightSpan{
	font-family: 'icomoon';
	font-size: 10px;
	padding: 0 10px;
}

.thumb li{
	position: relative;
}

.thumb li img{
	opacity: .25;
	padding: 3px;
	background: transparent;

	-webkit-transition : all .2s ease-out;
	-moz-transition : all .2s ease-out;
	-ms-transition : all .2s ease-out;
	-o-transition : all .2s ease-out;
	-khtml-transition : all .2s ease-out;
	transition : all .2s ease-out;
}

.thumb li img.active_thumb,
.thumb li img:hover{
	opacity: 1;

}

.thumb li img.active_border{
	background: white;

}


.thumb li img:hover{
	cursor: pointer;
	background: white;
}

.carrouselDetails button.carrouselLeft{
	left: 0;
}


.carrouselDetails button.carrouselRight{
	right: 0;
}

.align_carousel{
	width: 90%;
	margin: 0 auto;
	overflow: hidden;
}

.carrouselDetailsThumb{
	position: relative;
}

.content_thumb{
	position: relative;

	-webkit-transition : all .4s ease-out;
	-moz-transition : all .4s ease-out;
	-ms-transition : all .4s ease-out;
	-o-transition : all .4s ease-out;
	-khtml-transition : all .4s ease-out;
	transition : all .4s ease-out;
}

.thumb{
	padding: 10px 0;
	margin: 0 auto;
	overflow: auto;
}

.thumb li{
	float: left;
	padding: 10px 0;
	margin-right: 10px;
}

.thumb li img{
	width: auto;
	height: 88px;
}

.info_picture{
	height: 72%;
	position: relative;
}

.info_picture li{
	width: 100%;
	height:100%;
	float: left;
	position: absolute;
	opacity: 0;
	color: #cccccc;

	-webkit-transition: all .3s ease-out;
	-moz-transition: all .3s ease-out;
	-ms-transition: all .3s ease-out;
	-o-transition: all .3s ease-out;
	-khtml-transition: all .3s ease-out;
	transition: all .3s ease-out;
}

.button_collection_details{
	position: absolute;
	bottom: 0;
	right: 0;
	border: none;
	background: #383838;
	font-size: 22px;
	width: 20%;
	padding: 46.5px 0;
	text-indent: 38px;
	z-index: 20000;
	color:white;
}


span.buttonSpan{
	display: block;
	width: 34px;
	height: 34px;
	background: url(../images/details/cross.png);
	background-size: 100%;
	background-position: center;
	position: absolute;
	left: 38px;

	-webkit-transition : all .2s ease-out;
	-moz-transition : all .2s ease-out;
	-ms-transition : all .2s ease-out;
	-o-transition : all .2s ease-out;
	-khtml-transition : all .2s ease-out;
	transition : all .2s ease-out;
}

span.buttonSpan.active_span{

	-webkit-transform:rotate(45deg) !important;
	-webkit-transition : all .2s ease-out;

	-moz-transform:rotate(45deg) !important;
	-moz-transition : all .2s ease-out;

	-ms-transform:rotate(45deg) !important;
	-ms-transition : all .2s ease-out;

	-o-transform:rotate(45deg) !important;
	-o-transition : all .2s ease-out;

	-khtml-transform:rotate(45deg) !important;
	-khtml-transition : all .2s ease-out;

	transform:rotate(45deg) !important;
	transition : all .2s ease-out;
}

.image_slide{
    overflow: hidden;
	width: 78%;
	height: 100%;
	padding-bottom: 108px;
	float: right;
	position: relative;
	background: #383838;

	-webkit-transition: all .5s ease-out;
	-moz-transition: all .5s ease-out;
	-ms-transition: all .5s ease-out;
	-o-transition: all .5s ease-out;
	-khtml-transition: all .5s ease-out;
	transition: all .5s ease-out;

}


.image_slide_content{
	position: absolute;
	width: 100%;
	height: 100%;
	right: 0;

	-webkit-transition: all .5s ease-out;
	-moz-transition: all .5s ease-out;
	-ms-transition: all .5s ease-out;
	-o-transition: all .5s ease-out;
	-khtml-transition: all .5s ease-out;
	transition: all .5s ease-out;
}

.image_slide_content ul li{
	position: relative;
	height: 100%;
	margin-right: 2px;
}

.image_slide_content ul li img{
	width: auto;
	height: 100%;
	display: block;
}

.image_slide ul{
	overflow: hidden;
	height: 100%;
}

.image_slide li{
	float: left;
}

.collection{
	width: 20%;
	position: absolute;
	right: 0;
	top: 100%;
	height: 100%;
	padding-bottom: 128px;

	-webkit-transition: all .3s ease-out;
	-moz-transition: all .3s ease-out;
	-ms-transition: all .3s ease-out;
	-o-transition: all .3s ease-out;
	-khtml-transition: all .3s ease-out;
	transition: all .3s ease-out;

}

.small_info a{
	display: block;
	color: #cccccc;
	width: 100%;
	height: 100%;
}

#scroller{
	background: #262626;
	padding-top: 41px;
}

.scroll_section{
	width: 100%;
	height: 100%;
}

.collection.top_none{
	top: 0;
}

.collection h3{
	display: block;
	width: 100%;
	padding: 14px 0;
