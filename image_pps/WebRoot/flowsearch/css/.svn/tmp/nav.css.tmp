@charset "utf-8";
/* CSS Document */

#topBar{
padding:0 30px;
position:relative;
z-index:999;
}


.pageSearch {
 
}

#topBar > .container{
padding-left:0px; 
padding-right:0px;
    position: relative;
}

.pageSearch .container {
	width:100%;
}

.navWrap:before,
.navWrap:after{
content: " ";
display: table;
clear:both;
}

.navWrap{
 padding:20px 0;
 /*position:relative;*/
 z-index:999;
}
.navWrap, .lightBoxWrap, .searchBar{

}

 
#navLeft{
/*display:none;*/
float:left;
/*position:relative;*/
z-index:2;
}

#navRight{
float:right;
/*position:relative;*/
    position: static;
z-index:2;
}


#navRight a{
line-height:50px;
display:inline-block;
height:50px;
padding:0 15px;
font-size:16px;
}

#navLeft a{
 line-height:55px;
display:block;
/*padding:0 15px;*/
font-size:16px;
    height: 100%;
    width: 100%;
    text-align: center;
}


#topBar li a.active{
 color:#6031ba;
}

#navRight li a:hover, #navRight li a.hoverShow.active{
background:#323232;
color:#fff;
}




#navLeft ul ul li:hover a{
background:#000;
}

#logo{
float:left;
}

.collapse{
width:50px;
height:50px;
line-height:50px;
float:left;
font-size:30px;
text-align:center;
cursor:pointer;
vertical-align:middle;
position:relative;
z-index:2;
}


#navLeft ul li {
float:left;
/*position:relative;*/
margin-right:10px;

}
#navLeft ul li.active:before {
    background:#000;
    content: "";
    display: block;
    height: 10px;
    left: 0;
    position: absolute;
    top: -20px;
    width: 100%;
}

#navLeft ul li:hover a {
    /*background:#323232;*/
    /*color:#fff;*/
}



#navLeft > ul > li > a{
-webkit-transition:none;
-moz-transition:none;
-o-transition:none;
transition:none;
    padding: 0 15px;
}


#navLeft ul ul{
position:absolute;
width:140px;
background:#323232;
padding:10px 0;
display:none;
}

#navLeft ul li:hover ul{
display:block;
}

#navLeft ul ul li{
margin:0;
float:none;
}

#navLeft ul ul li a{
font-size:14px;
color:#fff;
height:38px;
line-height:38px;
padding-left:20px;
padding-right:20px;
}

#navRight ul{
float:right;
}

#navRight ul li {
    float:left;
    position: relative;
    z-index: 2;

}
.pageHome #navRight ul li:nth-child(3){
    display:none;
}


.pageImageSetDetail #navRight ul li:nth-child(3){
    display:none;
}
#navRight ul li:first-child {
    float:left;
    position: absolute;
    left: 0;
    top: 0;

}

#navRight ul li a{
min-width:50px;
font-size:22px;
text-align:center;
padding:0 10px;
}
.logInName{
 font-size:11px;
 display:inline-block;
 line-height:20px;
 vertical-align:bottom!important;
 margin-bottom:10px;
font-family: 'Microsoft YaHei', Helvetica, sans-serif;
}

#logo a{
line-height:50px;
height:50px;
 
font-size:16px;
padding:0 45px 0 15px;
display:inline-block;
}

#logo img{
max-height:100%;
}

.collapse:hover,.expand .collapse{
background:#323232;
color:#fff;
}



.pageHome #navLeft{
    position: relative;
}

.pageSearch #navLeft.expand{
    position: static;

}


.pageHome #navLeft .secondaryMenu .container,
.pageSearch #navLeft .secondaryMenu .container {
    width: 1920px;
}
.pageHome #logo{
text-align:center;
position:absolute;
z-index:1;
left:0;
top:0;
width:100%;
/*height:100%;*/
margin:20px 0;
}


.secondaryMenu .classification {
    width: 180px;
    margin: 0 5px;
    display: block;
    float: left;
}



#topBar .searchBar{
 margin-bottom:30px;
}

#navRight .icon-11-favourite{
 position:relative; 
}

#navRight .icon-11-favourite.active:after{
 content:' ';
 display:block;
 width:50px;
 height:20px;
 background:url(../images/arrow.png) center bottom no-repeat;
 position:absolute;
 bottom:-20px;
 left:-0px;
}





 
.collapseFav{
font-size: 22px;
padding: 0;
text-align: center;
width: 50px;
float:right;
cursor:pointer;
height: 50px;
line-height: 50px;
display:block;
position:relative;
z-index:3;

}
.collapseFav:hover, .collapseFav.active{
 background:#323232;
 color:#fff;
}
#userCenterMobileMenu{

}

#userCenterMobileMenu{
position:absolute;
top:0;
right:0;
z-index:9999;
margin-top:70px;

color:#fff;
display:none;
}

#userCenterMobileMenu.expand{
 display:block;
}


#userCenterMobileMenu ul{
background:#fff;
min-width:160px!important;
}

#userCenterMobileMenu ul li a{
 padding:6px 0;
 display:block;
 color:#7d7d7d;
 margin-bottom:1px;
}


#userCenterMobileMenu ul li a:hover, #userCenterMobileMenu ul li.active{
 background:#000;
    color: #fff;
}
#userCenterMobileMenu ul li a:hover:before, #userCenterMobileMenu ul li.active:before{
    color: #fff
}


#userCenterMobileMenu a.iconUser:before,
#userCenterMobileMenu a.iconFolder:before,
#userCenterMobileMenu a.iconHistory:before,
#userCenterMobileMenu a.iconInvoice:before,
#userCenterMobileMenu a.iconSet:before,
#userCenterMobileMenu a.iconLogOut:before
{
 display:inline-block;
 font-size:22px;
 width:68px;
 text-align:center;
 color:#323232;
}


.secondaryMenu {
    display: block;
}
#navLeft ul li:hover{
    background: #323232;
    color: #fff;
}
#navLeft ul li a span.icon-font{
    display: none;
}
#navLeft ul li:hover a{
    color: #fff;
}
#navLeft ul li .classification a{
    color: #323232;
}
#navLeft ul li:hover .secondaryMenu {
    /*display: block;*/
}
#navLeft ul li:hover .secondaryMenu a {
    background: none;
}
#navLeft ul .secondaryMenu {
    position: fixed;
    top: 95px;
    left: 0;
    background: #fff;
    padding: 50px 0 0 0;
    display: none;
    margin-top: -20px;
    width: 100%;
    z-index: 334;
}
/*
#navLeft ul .secondaryMenu{filter:progid:DXImageTransform.Microsoft.gradient(enabled='true',startColorstr='#F2FFFFFF', endColorstr='#F2FFFFFF');background:rgba(255,255,255,0.95);}
:root #navLeft ul .secondaryMenu{filter:progid:DXImageTransform.Microsoft.gradient(enabled='true',startColorstr='#00FFFFFF', endColorstr='#00FFFFFF');}
*/


.hotAlbums{
    font-size: 12px;
    line-height: 50px;
}
.hotAlbums{filter:progid:DXImageTransform.Microsoft.gradient(enabled='true',startColorstr='#F2EBEBEB', endColorstr='#F2EBEBEB');background:rgba(235,235,235,0.95);}
:root .hotAlbums{filter:progid:DXImageTransform.Microsoft.gradient(enabled='true',startColorstr='#00EBEBEB', endColorstr='#00EBEBEB');}/*for IE9*/
.hotAlbums .label {
    font-size: 18px;
    color: #323232;
    position: absolute;
    left: 0;
}
.hotAlbums .col-md-12 {
    position: relative;
    padding-left: 120px;
    line-height: 50px;
}
#navLeft ul li .secondaryMenu .hotAlbums .navBadge {
    display: inline-block;
    width: auto;
    color: #323232;
    font-size: 12px;
    padding: 5px 12px;
    border-radius: 11px;
    line-height: 12px;
    margin: 14px 0;
}

/*#navLeft ul li:hover .secondaryMenu .hotAlbums .navBadge.active,*/
#navLeft ul li:hover .secondaryMenu .hotAlbums .navBadge:hover {
    background: #6031ba;
    color: #fff;
}
#navLeft .secondaryMenu span a{
    line-height: 44px;
}
#navLeft .secondaryMenu span a:last-child {
    margin-bottom: 16px;
}
#navLeft .navMobileSub{
    height: auto;
    padding: 0;
    margin-top: 50px;
    background: #323232;
    margin-left: 100%;
    display: none;
}
.navMobileSub dl{

}
.navMobileSub dt{
    line-height: 50px;
    width: 120px;
    border-right: 1px solid #2b2b2b;
    font-size: 12px;
}
.navMobileSub dd{
    border-top: 1px solid #2b2b2b;
}
#navLeft .navMobileSub dd a{
    color:#fff;
    text-align: left;
    text-indent: 18px;
    font-size: 12px;
}
#navLeft .navMobileSub dd a:hover{
    background: #6b6b6b;
}

.navMobileSub dt span{
    display: inline-block;
    width: 50px;
    line-height: 50px;
    text-align: center;
    cursor: pointer;
}

.secondaryMenu span a span:before{
    top: 56px;
}
.secondaryMenu span a span{
    line-height: 80px;
}.secondaryMenu span {
     display: inline-block;
 }
.secondaryMenu span a:first-child {
    position: relative;
}
.secondaryMenu span p {
    font-size: 0;
    line-height: 16px;
    border-top: 1px solid #f7f7f7;
    padding-top: 10px;
    text-align: center;
    margin-bottom: 20px;
    color: #848484;
    height: 60px;
}
.secondaryMenu span:hover p {
    font-size: 12px;
}
.secondaryMenu span:hover a span {
    display: inline-block;
}
.secondaryMenu span a:hover span{
    display: inline-block;
}
.secondaryMenu span a span{
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    line-height: 100px;
    color: #fff;
    display: none;
}

.secondaryMenu span a span{filter:progid:DXImageTransform.Microsoft.gradient(enabled='true',startColorstr='#BF000000', endColorstr='#BF000000');background:rgba(0,0,0,0.75);}
:root .secondaryMenu span a span{filter:progid:DXImageTransform.Microsoft.gradient(enabled='true',startColorstr='#00000000', endColorstr='#00000000');}/*for IE9*/


.secondaryMenu span a span:before{
    content: '';
    font-size: 0;
    display: block;
    width: 72px;
    position: absolute;
    top: 66px;
    background: #fff;
    height: 1px;
    left: 50%;
    margin-left: -36px;
}

.pageHome .hotAlbums,
.pageSearch .hotAlbums{
    padding: 0 5px;
}
.pageHome .navWrap {
    padding-left: 15px;
    padding-right:15px;
}


@media (max-width: 1920px) {
    .pageSearch #navLeft .secondaryMenu .container{
        width: 100%;
    }
    .pageHome #navLeft .secondaryMenu .container{
        width: 1180px;
    }
    .pageHome #navLeft .secondaryMenu .hotAlbums .container{
        width: 1170px;
    }
}

@media (max-width: 1200px) {

    .pageHome #navLeft .secondaryMenu .container{
        width: 970px;
    }
    .pageHome #navLeft .secondaryMenu .hotAlbums .container{
        width: 970px;
    }
}
@media (max-width: 991px) {

    /*display: block;*/
/*}#navLeft .navMobileSub {*/

    .pageHome #navLeft.expand{
    position: static;
    float: left;
    }

    #navRight ul{
        height: 50px;
    }
    #navRight ul li:last-child {
        position: absolute;
        right: 0;
        top: 0;
    }

#userCenterMobileMenu{
 width:100%;
 /*height:100%*/;
 background:#434343;
 /*position:fixed;*/
    position: absolute;
 top:0;
 left:0;
 z-index:9999;
 margin-top:49px;
 color:#fff;
-webkit-transition:all 0.3s;
-moz-transition:all 0.3s;
-o-transition:all 0.3s;
transition:all 0.3s;
}



#userCenterMobileMenu a{
 color:#fff;
}
#userCenterMobileMenu ul{
 margin:0;
 padding:0;
 overflow:hidden;
 position:relative;
 z-index:4;
 height:100%;
min-width:60px!important;
    padding-bottom: 40px;
}

#userCenterMobileMenu ul li{
 border-top:1px #2b2b2b solid;
 display:block;
 position:relative;
 min-width:280px;
}
    
    #userCenterMobileMenu ul{
        padding: 10px 0;
        background: #323232;
    }
#userCenterMobileMenu ul li a{
 padding:10px 0;
 color:#fff;
 margin-bottom:0;
    background: #323232;
}
#userCenterMobileMenu ul li a.btnLogOut {
    background: #6031ba;
}
    #userCenterMobileMenu a.iconUser:before, #userCenterMobileMenu a.iconFolder:before, #userCenterMobileMenu a.iconHistory:before, #userCenterMobileMenu a.iconInvoice:before, #userCenterMobileMenu a.iconSet:before, #userCenterMobileMenu a.iconLogOut:before{
        color: #fff;
    }


#userCenterMobileMenu ul li a:hover, #userCenterMobileMenu ul li.active{
 background:#434343;
}



#userCenterMobileMenu ul li .icon-2-arrowRight{
  display:block;
  position:absolute;
  right:0;
  top:0;
  font-size:16px;
  padding:15px 20px;
  cursor:pointer;
  border-left:1px #323232 solid;
}
#userCenterMobileMenu ul li .icon-2-arrowRight:hover{
 background:#000;
}

.navMobileSub{
 background:#434343;
 width:100%;
height:100%;
 position:absolute;
 left:0;
 top:0;
 color:#fff;
 padding-left:90px;
 overflow:auto;
    display: none;
}

.favFolderControl{
 text-align:center;
 border-bottom:1px #393939 solid;
 overflow:hidden;
 padding:20px 0;
}
.favFolderControl span{
 display:inline-block;
  text-align:center;
 width:55px;
 height:20px;
 font-size:20px;
 line-height:20px;
 cursor:pointer;
}

.favFolderControl .icon-46-createNew{
  float:left;
}
.favFolderControl .icon-50-trash{
  float:right;
}
.favFolderControl span.active:before{
 display:inline-block;
 background:#00ad7d;
 color:#fff;
 font-size:12px;
 width:20px;
 height:20px;
-webkit-border-radius: 30px;
-moz-border-radius: 30px;
border-radius: 30px;
 line-height:20px;
content: "\e116";
}



.navMobileSub .favFolderBox{
 border-bottom:1px #393939 solid;
 border-top:0;
}

.navMobileSub .favFolderBox:hover, .navMobileSub .favFolderBox.active{
 background:none;
}

.navMobileSub .favFolderBox:before{
 display:none;
}
.navMobileSub .favFolderBox .folder{
padding:8px 60px 8px 0px;  
display:block;
width:100%;  
}
.navMobileSub .favFolderBox .btnClose{
right:0px;
top:0px;
width:55px;
line-height:60px;
height:100%;
display:none;
}
    .navMobileSub .favFolderBox:hover .btnClose{
        display: block;
    }


.favouriteFolderList.showBtnClose .btnClose{
 display:block;
}

.navMobileSub .favFolderBox .favFolderName{
 font-size:16px;
}




#topBar, .navWrap{
padding:0px;
}



#navLeft{
position:static;
float:right;
}


#navLeft, #navRight, #logo {
padding:0;
}

#navLeft ul li:hover ul{
display:none;
}

#navLeft ul{
position:absolute;
width:100%;
top:50px;
left:0;
background:#323232;
z-index:3;
overflow:hidden;
display:none;
}

#navLeft.expand ul{
 display:block;
 padding:10px 0;
}
#navLeft.expand ul ul{
 display:none;
}

#navLeft ul li:first-child{
/*margin-top:10px;*/
}

#navLeft ul li {
float: none;
margin-right: 0px;
    position: relative;
}



#navLeft ul li a{
/*width:100%;
color:#fff;*/
    width: 100%;
    color: #fff;
    text-align: left;
    text-indent: 30px;
}
#navLeft ul li:hover a{
background:#6b6b6b;
}

#navLeft ul li span {
    width: 55px;
    height: 55px;
    position: absolute;
    top: 0;
    right: 0;
    text-align: center;
    line-height: 55px;
    color: #fff;
    cursor: pointer;
}
#navLeft ul li span:hover {
    background: #000;
}

.pageHome #logo,.pageSearch #logo{
padding:0;
margin:0;
    text-align: center!important;

    width:100%;
}
#logo{
 /*text-align:left!important;*/
}
#logo a{
padding-left:15px;
padding-right:15px;
}

